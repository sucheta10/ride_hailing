-- ANALYTICS TABLE --

CREATE TABLE analytics (
    analytic_id INT AUTO_INCREMENT PRIMARY KEY,
    date DATE,
    total_rides INT,
    avg_fare DECIMAL(10, 2),
    total_revenue DECIMAL(10, 2),
    avg_rating DECIMAL(3, 2),
    UNIQUE (date)  -- Ensure each date has a unique entry
);



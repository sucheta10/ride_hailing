-- ROUTE TABLE --

CREATE TABLE route (
    route_id INT AUTO_INCREMENT PRIMARY KEY,
    ride_id INT,
    start_location VARCHAR(255),
    end_location VARCHAR(255),
    intermediate_points TEXT,
    duration INT,
    FOREIGN KEY (ride_id) REFERENCES ride(ride_id)
);




-- DRIVER TABLE --

CREATE TABLE driver (
driver_id INT PRIMARY KEY,
name VARCHAR(100),
license_number VARCHAR(50),
rating DECIMAL(3,2),
status ENUM('active','suspended','deleted'),
created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP
);

